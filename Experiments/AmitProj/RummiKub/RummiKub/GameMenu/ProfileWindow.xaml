<Window x:Class="RummiKub.ProfileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RummiKub"
        mc:Ignorable="d"
        Title="ProfileWindow" Height="350" Width="525">
    <StackPanel>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="316" Width="517">
            <TabItem Header="Games List" Margin="0,0,-60,0" Height="22" VerticalAlignment="Bottom">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="The Winner" Margin="61,0,-126,-2">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Participants" Margin="127,0,-192,-2">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </StackPanel>
</Window>
