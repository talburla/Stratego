<Window x:Class="RummiKub.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RummiKub"
        mc:Ignorable="d"
        Title="Menu" Height="350" Width="525">
    <StackPanel>
        <Button x:Name="Play" Content="Play" HorizontalAlignment="Left" Height="33" Margin="144,30,0,0" VerticalAlignment="Top" Width="233" />
        <Button x:Name="Instructions" Content="Instructions" HorizontalAlignment="Left" Height="33" Margin="144,30,0,0" VerticalAlignment="Top" Width="233" Click="Instructions_Click" />
        <Button x:Name="Profile" Content="Profile" HorizontalAlignment="Left" Height="33" Margin="144,30,0,0" VerticalAlignment="Top" Width="233" Click="Profile_Click" />
        <Button x:Name="Settings" Content="Settings" HorizontalAlignment="Left" Height="33" Margin="144,30,0,0" VerticalAlignment="Top" Width="233" />
        <Button x:Name="Exit" Content="Exit" HorizontalAlignment="Left" Height="33" Margin="18,30,0,0" VerticalAlignment="Top" Width="119" Click="Exit_Click" />
        
    </StackPanel>
</Window>
