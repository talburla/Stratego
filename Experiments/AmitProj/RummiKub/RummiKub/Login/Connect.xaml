<Window x:Class="RummiKub.Connect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RummiKub"
        mc:Ignorable="d"
        Title="Connect" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <Button  Content="Sign In" Margin="150,100,150,10" Click="Sign_In"/>

            <Button  Content="Sign Up" Margin="150,20,150,10" Click="Sign_Up"/>

        </StackPanel>
    </Grid>
</Window>
